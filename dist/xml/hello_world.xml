<ENSEMBLE NAME="Hello_World">

	<GLOBAL_PARAMETERS CLOCK_MODE="CLOCK_USER" PROCESS_MODE="BATCH" OSC="FALSE" />

  	<!-- ********** ENVIRONMENT AGENT ********** -->

    <ENVIRONMENT_AGENT_CLASS NAME="ENVIRONMENT" CLASS="ensemble.apps.hello_world.Hello_World_Environment">
        <WORLD CLASS="ensemble.apps.hello_world.Hello_World_World">
            <PARAM DIMENSIONS="3" /> 
            <PARAM STRUCTURE="continuous" />
            <PARAM FORM="cube:100:loop" />
        </WORLD>
    </ENVIRONMENT_AGENT_CLASS>
	
  <!-- ********** MUSICAL AGENT CLASSES ********** -->

	<MUSICAL_AGENT_CLASS NAME="AGENT" CLASS="ensemble.apps.hello_world.Hello_World_Agent" />
  
  <!-- ********** MUSICAL AGENT INSTANCES ********** -->

	<MUSICAL_AGENT NAME="creature" CLASS="AGENT" QUANTITY="1" />

</ENSEMBLE>
